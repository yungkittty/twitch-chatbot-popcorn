{
  "author": "yungkittty",
  "name": "twitch-chatbot-popcorn",
  "version": "0.9.0-alpha",
  "scripts": {
    "start:development": "concurrently \"sh ./scripts/start-react-development.sh\" \"sh ./scripts/start-electron-development.sh\"",
    "build-linux:production": "sh ./scripts/build-production.sh --linux",
    "build-macos:production": "sh ./scripts/build-production.sh --mac",
    "build-window:production": "sh ./scripts/build-production.sh --win",
    "eject-react": "react-scripts eject"
  },
  "main": "./public/index.electron.js",
  "homepage": "./",
  "dependencies": {
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-scripts": "3.1.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "wait-on": "^3.3.0"
  },
  "build": {
    "appId": "com.electron.twitch-chatbot-popcorn",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
